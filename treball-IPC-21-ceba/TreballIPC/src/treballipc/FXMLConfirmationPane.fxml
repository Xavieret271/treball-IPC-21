   <children>
      <Label text="S'ha enviat un codi de confirmacio al teu email" textFill="WHITE" />
      <Label text="Introdueix-lo aci" textFill="WHITE" />
      <TextField fx:id="confirmationCode" promptText="Codi de Confirmacio" />
